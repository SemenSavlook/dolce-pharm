{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2,
        "maxBOF": 1
      }
    ],
    "eol-last": "off",
    "max-len": [
      "warn",
      {
        "code": 140
      }
    ]
  }
}